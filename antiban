if not _G.isAntiBan then
    _G.isAntiBan = true
    while true do
            task.wait(1)
            pcall(function()
                for i,v in getconnections(game:GetService("ScriptContext").Error) do 
                    v:Disconnect() 
                end
                task.wait(0.1)
            end)       
        end
    end
else
    return 
end
