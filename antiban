if not _G.isAntiBan then
    _G.isAntiBan = true
    repeat task.wait(0.1)
        for i,v in getconnections(game:GetService("ScriptContext").Error) do 
            v:Disconnect() 
            task.wait(0.5)
        end       
    until true == false
else
    error("already executed")
    return 
end
